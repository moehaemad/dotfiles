URxvt.font: xft:DejaVu Sans Mono:style=Book:antialias=true:size=10:minspace=False, xft:PowerlineSymbols:antialias=true:size=10

URxvt.boldFont: xft:DejaVu Sans Mono:style=Bold:antialias=true:size=10:minspace=False, xft:PowerlineSymbols:antialias=true:size=10

! Disable boldness to be shown only by boldness not URxvt.color8 to URxvt.color15
URxvt.intensityStyles: false

! Remove funky/lame scroll bar used on default
URxvt.scrollstyle: plain
URxvt.scrollBar_right: true

! EXTENSIONS ================ Provided on default installation not custom ~/.config/urxvt/ext
URxvt.perl-ext-common: selection-to-clipboard,tabbed,font-size

! for tabbed, shift down=newtab, shift left&right=moveTOtabs, ctrl left&right=movetabs, ctrl d=closetab

! change font size on the fly
URxvt.keysym.C-Up: perl:font-size:increase
URxvt.keysym.C-Down: perl:font-size:decrease
URxvt.keysym.C-S-Down: perl:font-size:reset
URxvt.keysym.C-S-Up: perl:font-size:reset


! selection to clipboard options or use middlebutton for paste primary select or M+middle for clipboard
URxvt.clipboard.autocopy: true
URxvt.keysym.M-c: perl:clipboard:copy
URxvt.keysym.M-v: perl:clipboard:paste

! Transparency
! URxvt*depth: 32
URxvt*transparent: true
URxvt*shading: 10
! URxvt.background: [100]#1d1f21



